local menuButton = Instance.new("TextButton")
local open = false
menuButton.Text = ">"
menuButton.Size = UDim2.new(0, 22, 0, 50)
menuButton.Position = UDim2.new(0.94, 0, 0.744, 0)
menuButton.ZIndex = 4
